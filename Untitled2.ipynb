   "source": [
    "tx = ty = linspace (-8, 8, 41)';\n",
    "[xx, yy] = meshgrid (tx, ty);\n",
    "r = sqrt (xx .^ 2 + yy .^ 2) + eps;\n",
    "tz = sin (r) ./ r;\n",
    "mesh (tx, ty, tz);\n",
    "xlabel (\"tx\");\n",
    "ylabel (\"ty\");\n",
    "zlabel (\"tz\");\n",
    "title (\"3-D Sombrero plot\");"
   ]
